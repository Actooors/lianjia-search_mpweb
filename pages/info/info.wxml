<view class='carousel_div'>
    <swiper class="swiper" indicator-dots="true" autoplay="true" interval="5000" duration="1000">
      <block wx:for="{{item.CarouselImages}}">
        <swiper-item>
          <image src="{{LogoIp}}{{item}}" class="slide-image" mode='aspectFill' />
        </swiper-item>
      </block>
    </swiper>
  </view>